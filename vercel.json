{
  "builds": [
    { 
        "src": "vite.config.ts", 
        "use": "@vercel/static-build", 
        "config": { "distDir": "dist" } 
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}